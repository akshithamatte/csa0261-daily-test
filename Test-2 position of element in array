#include<stdio.h>
#include <conio.h>
int main()
{       
    int a[10000],i,n,key;
    printf("enter the elements : ");
    scanf("%d", &n);
    printf("enter the present elements: ");
    
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
     printf("Enter the key : ");
    scanf("%d", &key);
    for(i=0; i<n; i++)
    {
        if(a[i]==key)
        {
			printf("element present ");
            return 0;		 
        }
    }
	printf("element  not  present");
}
